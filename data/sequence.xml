<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Student File Numbers -->
        <record id="student_file_number_sequence" model="ir.sequence">
            <field name="name">Student File Number Sequence</field>
            <field name="code">student.student.sequence</field>
            <field name="prefix">STD/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>